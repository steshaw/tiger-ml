-- ../testcases/test47.tig --
/* This is legal.  The second type "a" simply hides the first one.
   Because of the intervening variable declaration, the two "a" types
   are not in the same  batch of mutually recursive types.
   See also test38 */
let
	type a = int
	var b := 4
	type a = string
in
	0
end
--
../testcases/test47.tig:
LetExp([
 TypeDec[
  (a,
   NameTy(int))],
 VarDec(b,true,NONE,
  IntExp(4)),
 TypeDec[
  (a,
   NameTy(string))]],
 SeqExp[
  IntExp(0)])

